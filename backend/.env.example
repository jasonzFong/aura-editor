# Copy this file to .env and fill in your configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=aura
SECRET_KEY=your-secret-key-here

# AI Provider Configuration
# Choose provider: deepseek or openai
AI_PROVIDER=deepseek

# DeepSeek API (required if AI_PROVIDER=deepseek)
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# OpenAI API (required if AI_PROVIDER=openai)
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Specify model (leave empty for provider default)
# DeepSeek: deepseek-chat, deepseek-coder
# OpenAI: gpt-4, gpt-4-turbo, gpt-3.5-turbo
AI_MODEL=
